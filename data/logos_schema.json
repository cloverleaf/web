{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"patternProperties": {
		"^[^\"\/\\\n]+$": {
			"properties": {
				"alias": {
					"type": "string"
				},
				"logo": {
					"type":"string",
					"pattern": "logos\/[a-zA-Z0-9_\\-\\!]+\\.(png|jpg)"
				},
				"mini":{
					"type": ["string", "boolean"],
					"pattern": "logos\/[a-zA-Z0-9_\\-\\!]+-MINI\\.(png|jpg)"
				}
			},
			"type": "object",
			"additionalProperties": false
		}
	}
}